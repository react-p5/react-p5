{
  "name": "@react-p5/core",
  "description": "This Component lets you integrate p5 Sketches into your React App",
  "version": "1.0.0",
  "author": "Xander Low <xander@xandydandy.com>",
  "repository": "https://github.com/react-p5/react-p5.git",
  "homepage": "https://github.com/react-p5/react-p5",
  "bugs": "https://github.com/react-p5/react-p5/issues",
  "keywords": [
    "react-p5",
    "p5",
    "p5js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/react-p5.umd.js",
  "module": "./dist/react-p5.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-p5.es.js",
      "require": "./dist/react-p5.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "watch": "tsc && vite build --watch",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "dependencies": {
    "p5": "^1.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/p5": "^1.4.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.24.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jsdom": "^20.0.1",
    "path": "^0.12.7",
    "p5.js-svg": "^1.3.3",
    "typescript": "^4.6.4",
    "vite": "^3.1.7",
    "vite-plugin-dts": "^1.6.4",
    "vitest": "^0.24.1"
  },
  "peerDependencies": {
    "p5": "1.4.x",
    "react": "18.x",
    "react-dom": "18.x",
    "p5.js-svg": "^1.3.3"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "prettier": "../../prettier",
  "eslintConfig": {
    "extends": "../../eslint"
  }
}
