{
  "name": "react-p5",
  "private": true,
  "scripts": {
    "build": "turbo run build lint",
    "dev": "turbo run dev --filter=nextjs-example",
    "clean": "rm -rf ./apps/nextjs/.next",
    "watch": "turbo run watch",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "publish-packages": "turbo run build lint && changeset version && changeset publish",
    "postinstall": "pnpm run build",
    "nextjs": "pnpm --dir apps/nextjs",
    "core": "pnpm --dir packages/core",
    "utils": "pnpm --dir packages/utils",
    "sketch": "pnpm --dir packages/sketch"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "apps/*"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "8.34.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "prettier": "2.8.4",
    "turbo": "1.7.4",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@chakra-ui/react": "2.4.1",
    "@changesets/cli": "2.26.0",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "framer-motion": "9.0.2"
  },
  "prettier": "./prettier",
  "eslintConfig": {
    "extends": "./eslint"
  },
  "version": "0.0.2",
  "license": "MIT"
}
